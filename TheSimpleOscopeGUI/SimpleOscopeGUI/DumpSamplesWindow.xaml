<Window x:Class="SimpleOscope.DumpSamplesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SimpleOscope"
        mc:Ignorable="d"
        Title="DumpSamplesWindow" Height="300" Width="300">
    <Grid>
        <Button x:Name="ok_button" Content="Ok" HorizontalAlignment="Left" Height="34" Margin="55,150,0,0" VerticalAlignment="Top" Width="111" Click="ok_button_Click"/>
        <TextBox x:Name="file_name_textbox" HorizontalAlignment="Left" Height="37" Margin="55,106,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="184"/>
        <Label x:Name="label" Content="Dump Samples To File" HorizontalAlignment="Left" Height="38" Margin="32,6,0,0" VerticalAlignment="Top" Width="222" FontSize="21.333"/>
        <Label x:Name="label1" Content="File Name:" HorizontalAlignment="Left" Height="27" Margin="55,75,0,0" VerticalAlignment="Top" Width="128"/>

    </Grid>
</Window>
